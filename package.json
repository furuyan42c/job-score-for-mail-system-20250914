{
  "name": "super-claude-base-env",
  "version": "1.0.0",
  "description": "Super Claude + Spec-Kit Base Development Environment",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:chrome": "playwright test --project=chromium",
    "test:e2e:firefox": "playwright test --project=firefox",
    "test:e2e:safari": "playwright test --project=webkit",
    "test:e2e:mobile": "playwright test --project=\"Mobile Chrome\"",
    "test:e2e:report": "playwright show-report",
    "test:e2e:install": "playwright install",
    "test:all": "npm run test && npm run test:e2e",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"**/*.{js,ts,json,md}\"",
    "format:check": "prettier --check \"**/*.{js,ts,json,md}\"",
    "typecheck": "tsc --noEmit",
    "build": "cd front && npm run build",
    "dev": "cd front && npm run dev",
    "front:install": "cd front && npm install",
    "front:dev": "cd front && npm run dev",
    "front:build": "cd front && npm run build",
    "front:start": "cd front && npm run start"
  },
  "keywords": [
    "super-claude",
    "spec-kit",
    "claude-code",
    "mcp",
    "development"
  ],
  "author": "Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/furuyan42c/spek-kit-superclaude-env.git"
  },
  "bugs": {
    "url": "https://github.com/furuyan42c/spek-kit-superclaude-env/issues"
  },
  "homepage": "https://github.com/furuyan42c/spek-kit-superclaude-env#readme",
  "devDependencies": {
    "@playwright/test": "^1.55.0",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jest": "^27.6.0",
    "jest": "^29.7.0",
    "playwright": "^1.40.0",
    "prettier": "^3.1.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.3.0"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.57.4"
  }
}
